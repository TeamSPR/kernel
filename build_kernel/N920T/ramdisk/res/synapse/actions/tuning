# By UpInTheAir for SkyHigh kernels
# Interactive performance, battery & turbo values by g.lewarne @ XDA

BB=/sbin/busybox;

if [ "$($BB mount | grep rootfs | cut -c 26-27 | grep -c ro)" -eq "1" ]; then
	$BB mount -o remount,rw /;
fi;

case $1 in
hmp)
  P=/res/synapse/SkyHigh/tuning_hmp
  if [ ! -z "$2" ]; then
  if [ "$2" == Default ]; then
	chmod 0644 /sys/kernel/hmp/up_threshold;
	echo "479" > /sys/kernel/hmp/up_threshold;
	chmod 0440 /sys/kernel/hmp/up_threshold;
	chmod 0644 /sys/kernel/hmp/down_threshold;
	echo "214" > /sys/kernel/hmp/down_threshold;
	chmod 0440 /sys/kernel/hmp/down_threshold;
    echo 0 > $P
  elif [ "$2" == Performance ]; then
	chmod 0644 /sys/kernel/hmp/up_threshold;
	echo "400" > /sys/kernel/hmp/up_threshold;
	chmod 0440 /sys/kernel/hmp/up_threshold;
	chmod 0644 /sys/kernel/hmp/down_threshold;
	echo "150" > /sys/kernel/hmp/down_threshold;
	chmod 0440 /sys/kernel/hmp/down_threshold;
    echo 1 > $P
  elif [ "$2" == Battery ]; then
    echo 2 > $P
	chmod 0644 /sys/kernel/hmp/up_threshold;
	echo "700" > /sys/kernel/hmp/up_threshold;
	chmod 0440 /sys/kernel/hmp/up_threshold;
	chmod 0644 /sys/kernel/hmp/down_threshold;
	echo "256" > /sys/kernel/hmp/down_threshold;
	chmod 0440 /sys/kernel/hmp/down_threshold;
  fi; fi
  copy=$(cat $P)
  if [ "$copy" == 0 ]; then
    echo Default
  elif [ "$copy" == 1 ]; then
    echo Performance
  elif [ "$copy" == 2 ]; then
    echo Battery
  fi
  ;;
interactive_turbo)
  P=/res/synapse/SkyHigh/tuning_interactive_turbo
  CPU0=$(cat /sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_min_freq);
  CPU4=$(cat /sys/devices/system/cpu/cpu4/cpufreq/cpuinfo_min_freq);
  if [ ! -z "$2" ]; then
  if [ "$2" == Disabled ]; then
    echo 0 > $P
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/enforced_mode;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/enforced_mode;
	echo "$CPU0" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/single_cluster0_min_freq;
	echo "$CPU4" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/single_cluster0_min_freq;
  elif [ "$2" == Enabled ]; then
    echo 1 > $P
	echo "$CPU0" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/single_cluster0_min_freq;
	echo "$CPU0" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/single_cluster0_min_freq;
	echo "1" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/enforced_mode;
	echo "1" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/enforced_mode;
  fi; fi
  copy=$(cat $P)
  if [ "$copy" == 0 ]; then
    echo Disabled
  elif [ "$copy" == 1 ]; then
    echo Enabled
  fi
  ;;
interactive_b)
  P=/res/synapse/SkyHigh/tuning_interactive_b
  if [ ! -z "$2" ]; then
  if [ "$2" == Default ]; then
    echo 0 > $P
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "59000 1300000:39000 1700000:19000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "40000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "89" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "65 1500000:75" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "40000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time;
	echo "20000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_rate;
  elif [ "$2" == Performance ]; then
    echo 1 > $P
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "25000 1300000:20000 1700000:20000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "45000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "83" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "1" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "60 1500000:70" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "35000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time;
	echo "15000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_rate;
  elif [ "$2" == Battery ]; then
    echo 2 > $P
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "70000 1300000:55000 1700000:55000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay;
	echo "25000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "95" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "80 1500000:90" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads;
	echo "15000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time;
	echo "15000" > /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_rate;
  fi; fi
  copy=$(cat $P)
  if [ "$copy" == 0 ]; then
    echo Default
  elif [ "$copy" == 1 ]; then
    echo Performance
  elif [ "$copy" == 2 ]; then
    echo Battery
  fi
  ;;
interactive_L)
  P=/res/synapse/SkyHigh/tuning_interactive_L
  if [ ! -z "$2" ]; then
  if [ "$2" == Default ]; then
    echo 0 > $P
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "1900" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "40000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "85" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "75" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "40000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time;
	echo "20000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate;
  elif [ "$2" == Performance ]; then
    echo 1 > $P
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "12000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "45000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "80" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "1" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "70" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "35000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time;
	echo "20000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate;
  elif [ "$2" == Battery ]; then
    echo 2 > $P
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "37000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay;
	echo "25000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "80" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy;
	chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "90" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	chmod 0440 /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads;
	echo "15000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time;
	echo "15000" > /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate;
  fi; fi
  copy=$(cat $P)
  if [ "$copy" == 0 ]; then
    echo Default
  elif [ "$copy" == 1 ]; then
    echo Performance
  elif [ "$copy" == 2 ]; then
    echo Battery
  fi
  ;;
ktoonservative)
  P=/res/synapse/SkyHigh/tuning_ktoonservative
  if [ ! -z "$2" ]; then
  if [ "$2" == User ]; then
    echo 0 > $P
	# Nothing to do here
  elif [ "$2" == Default ]; then
    echo 1 > $P
	echo "1" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_offline_screen_off;
	echo "1" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_offline_screen_off;
	echo "10" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_offline_screen_on;
	echo "10" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_offline_screen_on;
	echo "10" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_online_screen_off;
	echo "10" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_online_screen_off;
	echo "2" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_online_screen_on;
	echo "2" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_online_screen_on;
	echo "8" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_raise_screen_off;
	echo "8" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_raise_screen_off;
	echo "2" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/block_cycles_raise_screen_on;
	echo "2" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/block_cycles_raise_screen_on;
	echo "52" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off;
	echo "52" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off;
	echo "52" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on;
	echo "52" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/disable_hotplug;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/disable_hotplug;
	echo "40" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_1;
	echo "40" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_1;
	echo "45" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_2;
	echo "45" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_2;
	echo "50" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_3;
	echo "50" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_3;
	echo "55" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_4;
	echo "55" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_4;
	echo "60" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_5;
	echo "60" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_5;
	echo "65" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_6;
	echo "65" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_6;
	echo "70" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_7;
	echo "70" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_off_hotplug_7;
	echo "43" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_1;
	echo "43" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_1;
	echo "47" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_2;
	echo "47" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_2;
	echo "55" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_3;
	echo "55" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_3;
	echo "60" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_4;
	echo "60" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_4;
	echo "62" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_5;
	echo "62" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_5;
	echo "65" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_6;
	echo "65" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_6;
	echo "70" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_7;
	echo "70" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/down_threshold_screen_on_hotplug_7;
	echo "8" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/freq_step_lower_screen_off;
	echo "8" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/freq_step_lower_screen_off;
	echo "4" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/freq_step_lower_screen_on;
	echo "4" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/freq_step_lower_screen_on;
	echo "2" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/freq_step_raise_screen_off;
	echo "2" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/freq_step_raise_screen_off;
	echo "5" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/freq_step_raise_screen_on;
	echo "5" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/freq_step_raise_screen_on;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/ignore_nice_load;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/ignore_nice_load;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_1;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_1;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_2;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_2;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_3;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_3;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_4;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_4;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_5;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_5;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_6;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_6;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_7;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_off_core_7;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_1;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_1;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_2;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_2;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_3;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_3;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_4;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_4;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_5;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_5;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_6;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_6;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_7;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/lockout_hotplug_screen_on_core_7;
	echo "1" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/no_extra_cores_screen_off;
	echo "1" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/no_extra_cores_screen_off;
	echo "45000" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/sampling_rate;
	echo "45000" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/sampling_rate;
	echo "40000" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/sampling_rate_screen_off;
	echo "40000" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/sampling_rate_screen_off;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/super_conservative_screen_off;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/super_conservative_screen_off;
	echo "0" > /sys/devices/system/cpu/cpu0/cpufreq/ktoonservative/super_conservative_screen_on;
	echo "0" > /sys/devices/system/cpu/cpu4/cpufreq/ktoonservative/super_conservative_screen_on;
